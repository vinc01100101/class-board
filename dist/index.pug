html
 head
  title GATE
  link(rel="stylesheet", href="./style.css")
 body
  h1 Single Page
  div(id='currentPage') #{currentPage}
  div(id='errorDom') #{errorDom}
  div(id='successDom') #{successDom}
  div(id='schools') #{schools}<!-- Homepage School List JSON-->
  
  div(id='userProfile') #{userProfile} <!-- User's JSON-->
  div(id='schoolPageLayout') #{schoolPageLayout} <!-- SchoolPage custom style JSON-->
  div(id='listOfAdmins') #{listOfAdmins} <!-- List of admins -->

  div(id='root')
  script(src='./gate-bundle.js')
